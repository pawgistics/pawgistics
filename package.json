  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "cross-env NODE_ENV=development nodemon -e js,jsx --ignore build --ignore dist --exec babel-node src/server",
    "dev:wds": "cross-env NODE_ENV=development webpack-dev-server --hot --mode development --progress",
    "prod:build": "rimraf build dist && babel src/server -d build/server --copy-files --ignore .example.json && cross-env NODE_ENV=production webpack --mode production --progress",
    "prod:start": "cross-env NODE_ENV=production node build/server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
