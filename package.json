{
  "name": "pawgistics",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "cross-env NODE_ENV=development nodemon -e js,jsx --ignore build --ignore dist --exec babel-node src/server",
    "dev:wds": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "prod:build": "rimraf build dist && babel src -d build --copy-files --ignore .test.js,.example.json && cross-env NODE_ENV=production webpack --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start build/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
